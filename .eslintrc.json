{
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "camelcase": 0,
    "comma-dangle": 0,
    "consistent-return": 0,
    "max-len": ["error", 160],
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "no-unused-expressions": 0,
    "prettier/prettier": "error"
  },
  "env": {
    "node": true,
    "jest": true
  }
}
